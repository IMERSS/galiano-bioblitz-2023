#!/bin/bash
# Copies tabular data and plot data generated by knitting R markdown docs into imerss-bioinfo repository for processing
# Note this is dependent on particular git checkout structure on a particular machine
find tabular_data/ -type f -name '*_records_gridded.csv' ! -name '*_new_records*' ! -name '*_confirmed_records*' ! -name '*_reported_records*' ! -name '*_summary*' -exec cp -v {} ../bagatelle/data/Galiano\ 2023/ \;
cp viz_data/*PlotData.json ../bagatelle/data/Galiano\ 2023/
